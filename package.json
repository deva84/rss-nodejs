{
  "name": "react-redux-typescript",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.40",
    "jest-junit": "16.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "react-router-dom": "^6.28.2",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.15.0",
